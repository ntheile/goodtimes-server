{
  "name": "goodtimes",
  "version": "1.0.0",
  "description": "goodtimes server",
  "main": "server/index.js",
  "author": "Nick Theile",
  "license": "MIT",
  "engines": {
    "node": "10.16.0",
    "yarn": "1.17.3"
  },
  "scripts": {
    "dev": "set NODE_ENV=development && npm run babel-node server/start.js ",
    "startnew": "NODE_ENV=development npm run babel-node server/start.js",
    "debug": "NODE_ENV=production && npm run babel-node  --debug -- server/start.js --debug --inspect-brk",
    "start": "NODE_ENV=production && npm run babel-node server/start.js --inspect-brk",
    "now-build": "NODE_ENV=production && yarn babel-node server/start.js",
    "startlinux": "NODE_ENV=production yarn babel-node server/start.js",
    "nodemon": "nodemon -x 'npm run start'",
    "build": "next build",
    "heroku-postbuild": "next build",
    "live-logging": "heroku logs --app goodtimes-server --tail",
    "lint": "eslint ./common/**/*.js ./components/**/*.js ./pages/**/*.js --fix",
    "prettier": "prettier --config .prettierrc ./common/**/*.js ./components/**/*.js ./pages/**/*.js",
    "babel-node": "babel-node --config-file ./babel-server.config.js"
  },
  "dependencies": {
    "@awaitjs/express": "^0.3.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@small-tech/express-ws": "^1.0.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "bip32": "^2.0.4",
    "bip32-utils": "^0.11.1",
    "bip39": "^3.0.2",
    "bitcoinjs-lib": "^3.2.0",
    "blockstack": "19.0.0-alpha.3",
    "blockstack-ui": "^0.0.71",
    "core-js": "^2.6.10",
    "dotenv": "^6.0.0",
    "esm": "^3.2.25",
    "express": "^4.16.3",
    "express-force-https": "^1.0.0",
    "grid-styled": "^5.0.2",
    "hash-handler": "^2.0.1",
    "latlon-geohash": "^2.0.0",
    "linkifyjs": "^2.1.8",
    "localstorage-polyfill": "^1.0.1",
    "lodash": "^4.17.11",
    "mdi-react": "^5.2.0",
    "moment": "^2.22.2",
    "nano-css": "^3.5.0",
    "next": "^8.0.0",
    "next-nprogress": "^1.4.0",
    "node-fetch": "^2.6.0",
    "nodemailer": "^5.1.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nookies": "^2.0.1",
    "nprogress": "^0.2.0",
    "polished": "^3.0.0",
    "prop-types": "^15.6.2",
    "qs": "^6.6.0",
    "radiks": "0.2.0-alpha.4",
    "radiks-server": "0.1.6",
    "react": "^16.8.2",
    "react-cookie": "^3.0.8",
    "react-dom": "^16.8.2",
    "react-emailify": "^0.0.1",
    "react-powerplug": "^1.0.0",
    "react-use": "^5.4.1",
    "react-visibility-sensor": "^5.0.2",
    "rebass": "^2.3.4",
    "styled-components": "^4.1.3",
    "styled-system": "^3.1.6",
    "universal-cookie-express": "^3.0.7",
    "webpack": "4.28.4",
    "window": "^4.2.6",
    "wolfy87-eventemitter": "^5.2.8"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/register": "^7.7.0",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-scope": "^3.7.1",
    "nodemon": "^2.0.2",
    "prettier": "^1.16.4"
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "./",
      "../radiks/src",
      "../server"
    ],
    "ignore": [
      ".next/*",
      ".next",
      "pages/*",
      "components/*",
      "styled/*",
      "scripts/*"
    ]
  }
}
